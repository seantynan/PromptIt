{
  "manifest_version": 3,
  "name": "Prettifier",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["https://api.openai.com/*"],
  "action": {
    "default_title": "Prettify Selection"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "all_frames": true
    }
  ]
}
